{
  "name": "@kosyanmedia/devcom-spec-blueprint",
  "version": "1.0.0",
  "packageManager": "yarn@4.9.2",
  "description": "The generator helping to organize spec project.",
  "main": "index.js",
  "repository": "https://github.com/KosyanMedia/devcom-spec-blueprint.git",
  "author": "Devcom <devcom@aviasales.ru>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "yarn workspace @kosyanmedia/devcom-spec-app build",
    "start": "yarn workspace @kosyanmedia/devcom-spec-app start:prod",
    "start:dev": "yarn workspace @kosyanmedia/devcom-spec-app start:dev",
    "lint": "yarn workspaces foreach --all run lint",
    "heroku-postbuild": "yarn build && yarn workspaces focus --all --production",
    "postinstall": "husky || true",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@commitlint/types": "18.6.1",
    "@eslint/js": "^9.39.2",
    "@innocells/branch-naming-check": "1.0.0",
    "@kosyanmedia/eslint-config-devcom": "^2.4.8",
    "@types/node": "^20.17.19",
    "commitlint-plugin-regex-match": "^1.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-project-structure": "^3.14.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "15.4.3",
    "pinst": "^3.0.0",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.49.0"
  },
  "resolutions": {
    "@types/react": "^18.3.20",
    "@types/react-dom": "~18.3.6"
  }
}
